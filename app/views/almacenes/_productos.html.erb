<!--- Sublistado de productos de un almacen de distribuidora --->
<%= cabecera_sublistado "Almacenes del Producto de Distribuidora", "inventario_almacen", params[:update] %>
<% for objeto in @pexa %>
  <div id="<%= params[:update] %>_linea_<%= objeto.id %>" class="fila" >
    <%= fila_sublistado objeto %>
  </div>
<% end %>
<%= final_sublistado %>
<br/>

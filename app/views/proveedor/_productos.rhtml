<!--- Sublistado de productos pedidos a un proveedor --->

<%= cabecera_sublistado "Productos Comprados", ["albaran.fecha","producto.nombre","cantidad","albaran.codigo","albaran.factura.codigo"], params[:update] %>
<% i = 0 %>
<% for objeto in @lineas %>
  <div id="<%= params[:update] %>_producto_<%= (i += 1).to_s %>" class="fila" >
    <%= fila_sublistado objeto %>
  </div>
<% end %>
<%= final_sublistado %>
<br/>


<!--- Caja con info del filtrado --->

<% if session[("filtrado_fecha_inicio").to_sym] && session[("filtrado_fecha_fin").to_sym] != '' %>
  <fieldset id="filtro_libro_valores">
  <div class='linea' id="texto_filtrado">
      Filtro de fechas => desde "<%= session[("filtrado_fecha_inicio").to_sym] %>" hasta "<%= session[("filtrado_fecha_fin").to_sym] %>"
    </div>
  </fieldset>
<% end %>


<h1>New albaran_linea</h1>

<% form_for(@albaran_linea) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :cantidad %><br />
    <%= f.text_field :cantidad %>
  </p>
  <p>
    <%= f.label :descuento %><br />
    <%= f.text_field :descuento %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', albaran_lineas_path %>